<!-- banner part start-->
<section class="banner_part">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-12">
                <div class="banner_slider owl-carousel">
                    {{#each data}}
                    <div class="single_banner_slider">
                        <div class="row">
                            <div class="col-lg-5 col-md-8">
                                <div class="banner_text">
                                    <div class="banner_text_iner">
                                        <h1>{{this.Banner.banner_heading}}</h1>
                                        <p>{{this.Banner.descriptions}}</p>
                                        <a href="/product" class="btn_2">EXPLORE NOW</a>
                                    </div>
                                </div>
                            </div>
                            <div class="banner_img d-none d-lg-block">
                                <img src="/Images/BannerImages/{{this.Banner.image}}" alt="">
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
                <div class="slider-counter"></div>
            </div>
        </div>
    </div>
</section>
<!-- banner part start-->

<!-- feature_part start-->
<section class="feature_part padding_top">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8">
                <div class="section_tittle text-center">
                    <h2>Featured Category</h2>
                </div>
            </div>
        </div>
        <div class="row align-items-center justify-content-between">
            {{!-- {{#each feature}} --}}
            <div class="col-lg-7 col-sm-6">
                <div class="single_feature_post_text">
                    <p>{{feature.[0].featured.descriptions}}</p>
                    <h3>{{feature.[0].featured.title}}</h3>
                    <a href="/product" class="feature_btn">EXPLORE NOW <i class="fas fa-play"></i></a>
                    <img src="/Images/featuredCategory/{{feature.[0].featured.image}}" alt="">
                </div>
            </div>
            {{!-- {{/each}} --}}
            <div class="col-lg-5 col-sm-6">
                <div class="single_feature_post_text">
                    <p>{{feature.[1].featured.descriptions}}</p>
                    <h3>{{feature.[1].featured.title}}</h3>
                    <a href="product" class="feature_btn">EXPLORE NOW <i class="fas fa-play"></i></a>
                    <img src="/Images/featuredCategory/{{feature.[1].featured.image}}" alt="">
                </div>
            </div>
            <div class="col-lg-5 col-sm-6">
                <div class="single_feature_post_text">
                    <p>{{feature.[2].featured.descriptions}}</p>
                    <h3>{{feature.[2].featured.title}}</h3>
                    <a href="/product" class="feature_btn">EXPLORE NOW <i class="fas fa-play"></i></a>
                    <img src="/Images/featuredCategory/{{feature.[2].featured.image}}" alt="">
                </div>
            </div>
            <div class="col-lg-7 col-sm-6">
                <div class="single_feature_post_text">
                    <p>{{feature.[3].featured.descriptions}}</p>
                    <h3>{{feature.[3].featured.title}}</h3>
                    <a href="/product" class="feature_btn">EXPLORE NOW <i class="fas fa-play"></i></a>
                    <img src="/Images/featuredCategory/{{feature.[3].featured.image}}" alt="">
                </div>
            </div>
        </div>
    </div>
</section>
<!-- upcoming_event part start-->

<!-- product_list start-->
<section class="product_list section_padding">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="section_tittle text-center">
                    <h2>awesome <span>shop</span></h2>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="product_list_slider owl-carousel">
                    <div class="single_product_list_slider">
                        <div class="row align-items-center justify-content-between">
                            {{#each product}}
                            <div class="col-lg-3 col-sm-6">
                                <div class="single_product_item">
                                    <img src="/Images/productImages/{{this.Images.[0].productImg.[0]}}" alt="">
                                    <div class="single_product_text">
                                        <h4>{{this.product_name}}</h4>
                                        <h3>Rs :{{this.price}}</h3>
                                        <a href="#" class="add_cart">+ add to cart<i class="ti-heart"></i></a>
                                    </div>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
</section>
<!-- product_list part start-->

<!-- awesome_shop start-->
<section class="our_offer section_padding">
    <div class="container">
            <div class="row align-items-center justify-content-between">
                <div class="col-lg-6 col-md-6">
                    <div class="offer_img">
                        <img src="/Images/IMG/NicePng_smartwatch-png_9091454.png" alt="">
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="offer_text">
                        <h2>Weekly Sale on
                            60% Off All Products</h2>
                        <div class="date_countdown">
                            <div id="timer">
                                <div id="days" class="date"></div>
                                <div id="hours" class="date"></div>
                                <div id="minutes" class="date"></div>
                                <div id="seconds" class="date"></div>
                            </div>
                        </div>
                        <div class="input-group">
                            <div class="input-group-append">
                                <a href="/product" class="input-group-text btn_2" id="basic-addon2">book now</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </div>
</section>
<!-- awesome_shop part start-->

<!-- product_list part start-->
<section class="product_list best_seller section_padding">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="section_tittle text-center">
                    <h2>Best Sellers <span>shop</span></h2>
                </div>
            </div>
        </div>
        <div class="row align-items-center justify-content-between">
            <div class="col-lg-12">
                <div class="best_product_slider owl-carousel">
                    {{#each product}}
                    <div class="single_product_item">
                        <img src="/Images/productImages/{{this.Images.[0].productImg.[0]}}" alt="">
                        <div class="single_product_text">
                            <h4>{{this.product_name}}</h4>
                            <h3>Rs :{{this.price}}</h3>
                        </div>
                    </div>
                    {{/each}}
                    
                </div>
            </div>
        </div>
    </div>
</section>
<!-- product_list part end-->
